{
  "name": "@hicaru/bearby-react",
  "version": "0.2.4",
  "main": "index.ts",
  "repository": "git@github.com:bearby-wallet/bearby-react.git",
  "author": "Hicaru <hicc0rus@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w"
  },
  "files": [
    "dist/*",
    "lib/*",
    "index.ts"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "browser": {
    "crypto": false
  },
  "keywords": [
    "massa",
    "web3",
    "bearby",
    "blockchain",
    "bitcoin",
    "bytes",
    "cryptography",
    "react"
  ],
  "peer-dependencies": {
    "react": ">=17.x"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^23.0.3",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/react": "^18.0.26",
    "rollup": "^3.5.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@hicaru/bearby.js": "^0.4.0"
  }
}
